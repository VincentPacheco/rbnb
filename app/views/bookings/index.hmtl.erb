<div class="container text-center">
  <h2>Welcome to the Bookings section</h2>
  <p><%= link_to "add a new booking", new_booking_path%></p>
  <% @bookings.each do |booking| %>
    <div class="row">
      <div class="col-sm-10 col-sm-offset-1">
        <h3><%= booking.name %></h3>
        <p><%= booking.category %></p>
        <%= link_to "show", booking_path(booking), class: "btn btn-primary" %>
      </div>
    </div>
  <% end %>
</div>