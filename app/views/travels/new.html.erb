<div class="container">
  <%= simple_form_for(@travel) do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :longdescription %>
    <%= f.input :duration %>
    <%= f.input :price %>
    <%= f.input :capacity %>
    <%= f.input :photo, as: :file %>
    <%= f.input :address %>
    <%= f.submit %>
  <% end %>
</div>
